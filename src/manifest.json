{
  "package": "fun.okschoollife.mibandpro.beta",
  "name": "Ok School Life",
  "versionName": "0.2.0.1",
  "versionCode": 0.1,
  "minPlatformVersion": 1000,
  "icon": "/common/assets/icon/icon-v4-final.png",
  "deviceTypeList": [
    "watch"
  ],
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.prompt"
    }
  ],
  "config": {
    "logLevel": "log",
    "designWidth": 480,
    "designHeight": 336
  },
  "permissions": [
    {
      "name": "system.storage"
    },
    {
      "name": "system.request"
    },
    {
      "name": "system.file"
    },
    {
      "name": "ACCESS_NETWORK_STATE"
    },
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    }
  ],
  "resources": {
    "files": [
      "$src/common/data/events.json"
    ]
  },
  "router": {
    "entry": "pages/load",
    "pages": {
      "pages/load": {
        "component": "load"
      },
      "pages/index": {
        "component": "index"
      },
      "pages/main": {
        "component": "main"
      },
      "pages/developer": {
        "component": "developer"
      },
      "pages/achievement": {
        "component": "achievement"
      }
    }
  }
}